SELECT 'Rentals: PID --> HID' AS FD, CASE WHEN EXISTS ( SELECT P1.PID FROM Rentals P1, Rentals P2 WHERE P1.PID = P2.PID AND P1.HID <> P2.HID ) THEN 'does not hold' ELSE 'MAY HOLD' END AS VALIDITY;
SELECT 'Rentals: PID --> PN' AS FD, CASE WHEN EXISTS ( SELECT P1.PID FROM Rentals P1, Rentals P2 WHERE P1.PID = P2.PID AND P1.PN <> P2.PN ) THEN 'does not hold' ELSE 'MAY HOLD' END AS VALIDITY;
SELECT 'Rentals: PID --> S' AS FD, CASE WHEN EXISTS ( SELECT P1.PID FROM Rentals P1, Rentals P2 WHERE P1.PID = P2.PID AND P1.S <> P2.S ) THEN 'does not hold' ELSE 'MAY HOLD' END AS VALIDITY;
SELECT 'Rentals: PID --> HS' AS FD, CASE WHEN EXISTS ( SELECT P1.PID FROM Rentals P1, Rentals P2 WHERE P1.PID = P2.PID AND P1.HS <> P2.HS ) THEN 'does not hold' ELSE 'MAY HOLD' END AS VALIDITY;
SELECT 'Rentals: PID --> HZ' AS FD, CASE WHEN EXISTS ( SELECT P1.PID FROM Rentals P1, Rentals P2 WHERE P1.PID = P2.PID AND P1.HZ <> P2.HZ ) THEN 'does not hold' ELSE 'MAY HOLD' END AS VALIDITY;
